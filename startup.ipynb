   "source": [
    "kernel=aiml.Kernel()\n",
    "kernel.learn(\"std-startup.xml\")\n",
    "kernel.respond(\"load aiml b\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8bd1e8bf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "> Human : hello\n"
     ]
    },
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "WARNING: No match found for input: hello\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "> Bot : \n",
      "> Human : hello there\n",
      "> Bot : Hey, im a bot\n"
     ]
    }
   ],
   "source": [
    "while True:\n",
    "    input_text=input(\"> Human : \")\n",
    "    response = kernel.respond(input_text)\n",
    "    print(\"> Bot : \"+response)"
